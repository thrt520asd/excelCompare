<Window x:Class="excelCompare.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:excelCompare"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="145*"/>
            <RowDefinition Height="11*"/>
        </Grid.RowDefinitions>
        <Button Name="btn1" Content="文件1" HorizontalAlignment="Center" VerticalAlignment="Center" Height="66" Width="121" Click="btn1_Click" Margin="0,0,300,0"/>
        <Button Name="btn2" Content="文件2" HorizontalAlignment="Center" Margin="300,0,0,0" VerticalAlignment="Center" Height="66" Width="121" Click="btn2_Click"/>
        <Button Name="compareBtn" Content="比较" HorizontalAlignment="Center" VerticalAlignment="Center" Height="66" Width="121" Click="compareBtn_Click" Margin="0,300,0,0"/>

    </Grid>
</Window>
